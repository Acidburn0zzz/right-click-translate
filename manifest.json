{
	"manifest_version": 2,
	"name": "Right-Click Translate",
	"version": "1.2",
	"description": "Right-Click to translate selected text using Google Translate",
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["contextMenus"],
	"icons": {
		"16": "icon16.png",
		"128": "icon128.png"
	},
	"homepage_url": "https://github.com/ericvan76/right-click-translate"
}